{
  "name": "idm",
  "version": "0.1.0",
  "description": "Identity Management Service",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LearnersGuild/idm.git"
  },
  "author": {
    "name": "Jeffrey Wescott",
    "email": "jeffrey@learnersguild.org"
  },
  "scripts": {
    "build": "mkdir -p dist && webpack --config config/webpack-$NODE_ENV-config.js",
    "db:create": "createdb idm_$NODE_ENV",
    "db:migrate": "./node_modules/.bin/babel-node ./db/migrate.js",
    "lint": "./node_modules/.bin/eslint --ext .js,.jsx .",
    "start": "npm run build & node server",
    "test": "NODE_ENV=test ./node_modules/.bin/jest"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/jest-css-modules",
    "testFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx"
    ],
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/react-dom",
      "<rootDir>/node_modules/react-addons-test-utils"
    ]
  },
  "keywords": [
    "idm",
    "swagger"
  ],
  "license": "UNLICENSED",
  "private": true,
  "bugs": {
    "url": "https://github.com/LearnersGuild/idm/issues"
  },
  "homepage": "https://github.com/LearnersGuild/idm#readme",
  "dependencies": {
    "babel": "^5.8.34",
    "babel-core": "^5.8.34",
    "babel-loader": "^5.3.3",
    "babel-runtime": "^5.8.34",
    "bluebird": "^3.0.5",
    "bootstrap": "twbs/bootstrap#v4.0.0-alpha",
    "bootstrap-loader": "0.0.5",
    "css-loader": "^0.23.0",
    "css-modules-require-hook": "^2.0.4",
    "dotenv": "^1.2.0",
    "ejs": "^2.3.4",
    "express": "^4.13.3",
    "extract-text-webpack-plugin": "^0.9.1",
    "lodash": "^3.10.1",
    "node-fetch": "^1.3.3",
    "node-sass": "^3.4.2",
    "passport": "^0.3.2",
    "passport-google-oauth": "^0.2.0",
    "pg": "^4.4.3",
    "pg-query": "^0.11.0",
    "postcss": "^5.0.12",
    "postcss-modules-extract-imports": "^1.0.0",
    "postcss-modules-local-by-default": "^1.0.0",
    "postcss-modules-scope": "^1.0.0",
    "postcss-modules-values": "^1.1.1",
    "postgrator": "^2.5.0",
    "react": "^0.14.3",
    "react-dom": "^0.14.3",
    "sass-loader": "^3.1.2",
    "serve-static": "^1.10.0",
    "style-loader": "^0.13.0",
    "supertest": "^1.1.0",
    "swagger-tools": "0.9.*",
    "webpack": "^1.12.9",
    "webpack-dev-middleware": "^1.4.0",
    "webpack-hot-middleware": "^2.5.0",
    "yamljs": "^0.2.4"
  },
  "devDependencies": {
    "babel-eslint": "^4.1.3",
    "babel-plugin-react-transform": "^1.1.1",
    "eslint": "^1.7.3",
    "eslint-config-airbnb": "^0.1.0",
    "eslint-plugin-react": "^3.6.3",
    "jest-cli": "^0.8",
    "jest-css-modules": "^1.0.0",
    "piping": "^0.3.0",
    "react-addons-test-utils": "^0.14.3",
    "react-transform-hmr": "^1.0.1"
  },
  "engines": {
    "node": "4.2.x"
  }
}
