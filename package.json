{
  "name": "idm",
  "version": "0.1.0",
  "description": "Identity Management Service",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LearnersGuild/idm.git"
  },
  "author": {
    "name": "Jeffrey Wescott",
    "email": "jeffrey@learnersguild.org"
  },
  "scripts": {
    "build": "mkdir -p dist && webpack --config config/webpack.js",
    "build:profile": "mkdir -p dist && webpack --profile --json --config config/webpack.js > tmp/webpack.json",
    "icons:fetch": "mkdir -p dist && curl -s https://brand.learnersguild.org/icontags > dist/icons-metadata.json",
    "db:create": "node ./scripts/dbCreate.js",
    "db:drop": "node ./scripts/dbDrop.js",
    "db:migrate:configure": "node ./scripts/dbConfig.js > ./db/database.json",
    "db:migrate": "npm run db:migrate:configure && node ./node_modules/.bin/rethink-migrate -r db",
    "db:migrate:up": "npm run db:migrate:configure && node ./node_modules/.bin/rethink-migrate -r db up",
    "db:migrate:down": "npm run db:migrate:configure && node ./node_modules/.bin/rethink-migrate -r db down",
    "data:users": "./node_modules/.bin/babel-node test/generateUsers",
    "lint": "./node_modules/.bin/xo",
    "start": "npm run icons:fetch && node server",
    "postinstall": "npm run build",
    "test:ci": "npm run db:create && npm run db:migrate:up && npm run test:cov",
    "test:cov": "npm run test:cov:run && npm run test:cov:send",
    "test:cov:run": "./node_modules/.bin/nyc --reporter=lcov npm run test:run",
    "test:cov:send": "./node_modules/.bin/codeclimate-test-reporter < ./coverage/lcov.info",
    "test:run": "NODE_ENV=test ./node_modules/.bin/ava",
    "test": "npm run lint && npm run test:run"
  },
  "keywords": [
    "idm"
  ],
  "license": "UNLICENSED",
  "private": true,
  "bugs": {
    "url": "https://github.com/LearnersGuild/idm/issues"
  },
  "homepage": "https://github.com/LearnersGuild/idm#readme",
  "dependencies": {
    "@learnersguild/idm-jwt-auth": "^1.0.0",
    "babel-cli": "^6.6.5",
    "babel-core": "^6.7.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-polyfill": "^6.6.1",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-2": "^6.5.0",
    "babel-runtime": "^6.6.1",
    "bourbon": "^4.2.6",
    "bourbon-neat": "^1.7.4",
    "bull": "^1.0.0-rc1",
    "chokidar": "^1.4.3",
    "config": "^1.21.0",
    "cookie-parser": "^1.4.1",
    "cors": "^2.7.1",
    "css-loader": "^0.23.0",
    "css-modules-require-hook": "^3.0.0",
    "dotenv": "^2.0.0",
    "ejs": "^2.3.4",
    "eslint-config-xo-react": "^0.5.0",
    "eslint-plugin-react": "^4.1.0",
    "express": "^4.13.3",
    "express-graphql": "^0.4.9",
    "express-sslify": "^1.0.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "google-libphonenumber": "^1.0.25",
    "graphql": "^0.4.18",
    "graphql-custom-types": "^0.3.0",
    "history": "^2.0.0",
    "isomorphic-fetch": "^2.2.1",
    "jsdom": "^8.0.4",
    "json-loader": "^0.5.4",
    "lodash": "^4.5.1",
    "minimist": "^1.2.0",
    "newrelic": "^1.28.2",
    "node-sass": "3.4.2",
    "normalize.css": "^3.0.3",
    "optimize-css-assets-webpack-plugin": "^1.3.0",
    "passport": "^0.3.2",
    "passport-github": "^1.1.0",
    "passport-google-oauth20": "^1.0.0",
    "postcss": "^5.0.17",
    "postcss-modules-extract-imports": "^1.0.0",
    "postcss-modules-local-by-default": "^1.0.0",
    "postcss-modules-scope": "^1.0.0",
    "postcss-modules-values": "^1.1.1",
    "raven": "^0.10.0",
    "raven-js": "^2.1.1",
    "react": "^0.14.3",
    "react-addons-css-transition-group": "^0.14.7",
    "react-dom": "^0.14.3",
    "react-redux": "^4.0.5",
    "react-router": "^2.0.0",
    "react-router-redux": "^4.0.0",
    "react-toolbox": "^0.15.0",
    "redux": "^3.0.5",
    "redux-auth-wrapper": "^0.3.0",
    "redux-form": "^4.2.0",
    "redux-thunk": "^1.0.3",
    "resolve-url-loader": "^1.4.3",
    "rethink-migrate": "^1.3.1",
    "rethinkdb": "2.3",
    "rethinkdbdash": "2.3",
    "sass-loader": "^3.2.0",
    "sass-resources-loader": "^1.0.2",
    "serve-static": "^1.10.0",
    "style-loader": "^0.13.0",
    "toolbox-loader": "0.0.3",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.14",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.10.0",
    "yamljs": "^0.2.5"
  },
  "devDependencies": {
    "ava": "^0.14.0",
    "babel-plugin-istanbul": "^1.0.3",
    "babel-plugin-react-transform": "^2.0.0-beta1",
    "bluebird": "^3.3.5",
    "codeclimate-test-reporter": "^0.3.3",
    "enzyme": "^2.3.0",
    "eslint": "^2.2.0",
    "eslint-config-xo-react": "^0.3.0",
    "eslint-plugin-react": "^3.16.1",
    "factory-girl": "^3.0.1",
    "faker": "^3.1.0",
    "jsdom": "^8.0.2",
    "nyc": "^7.1.0",
    "pre-commit": "^1.1.3",
    "react-addons-test-utils": "^0.14.3",
    "react-transform-hmr": "^1.0.1",
    "xo": "^0.12.1"
  },
  "engines": {
    "node": "5.6.x",
    "npm": "3.7.x"
  },
  "xo": {
    "extends": "xo-react",
    "rules": {
      "comma-dangle": [
        0
      ],
      "no-negated-condition": [
        0
      ],
      "react/jsx-indent": [
        2,
        2
      ],
      "react/jsx-indent-props": [
        2,
        2
      ],
      "quotes": [
        2,
        "single",
        "avoid-escape"
      ],
      "xo/filename-case": 0
    },
    "esnext": true,
    "space": true,
    "semicolon": false
  },
  "ava": {
    "files": [
      "**/__tests__/*-test.js"
    ],
    "failFast": true,
    "verbose": true,
    "require": [
      "babel-core/register",
      "babel-polyfill",
      "./config/testConfig"
    ],
    "babel": "inherit"
  },
  "nyc": {
    "sourceMap": false,
    "instrument": false
  },
  "precommit": [
    "lint"
  ]
}
